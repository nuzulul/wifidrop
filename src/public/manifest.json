{
  "id": "WIFIDrop",
  "background_color": "#ffffff",
  "description": "Instant WIFI Drop P2P Large File Transfer",
  "dir": "ltr",
  "display": "standalone",
  "name": "WIFIDrop",
  "orientation": "any",
  "scope": "/",
  "short_name": "WIFIDrop",
  "start_url": "/",
  "theme_color": "#646cff",
  "categories": [
    "social"
  ],
  "screenshots": [],
  "icons": [
      {
          "src": "/icon-512.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "any"
      }
  ],
  "capture_links": "existing-client-navigate",
  "url_handlers": [
    {
      "origin": "https://wifidrop.js.org"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+wifidrop",
      "url": "/?wifidrop=%s"
    }
  ],
  "intent_filters": [
    {
      "scope_url_scheme": "https",
      "scope_url_host": "wifidrop.js.org",
      "scope_url_path": "/"
    }
  ],
  "related_applications": [{
    "platform": "webapp",
    "url": "https://wifidrop.js.org/manifest.json"
  }],
  "lang": "id",
  "launch_handler": {
      "client_mode": ["navigate-existing", "auto"]
  },  
  "screenshots" : [
    {
      "src": "screenshot.jpeg",
      "sizes": "794x748",
      "type": "image/jpeg",
      "platform": "wide"
    }
  ]

}